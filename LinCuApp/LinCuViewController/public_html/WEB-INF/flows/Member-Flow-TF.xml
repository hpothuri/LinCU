<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="Member-Flow-TF">
    <default-activity>MemberInfo</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <view id="MemberInfo">
      <page>/fragments/MemberInfo.jsff</page>
    </view>
    <view id="MemberKYC">
      <page>/fragments/MemberKYC.jsff</page>
    </view>
    <method-call id="CreateInsert">
      <method>#{bindings.CreateInsert.execute}</method>
      <outcome>
        <fixed-outcome>CreateInsert</fixed-outcome>
      </outcome>
    </method-call>
    <control-flow-rule id="__3">
      <from-activity-id>MemberKYC</from-activity-id>
      <control-flow-case id="__4">
        <from-outcome>back</from-outcome>
        <to-activity-id>MemberInfo</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__1">
      <from-activity-id>MemberInfo</from-activity-id>
      <control-flow-case id="__2">
        <from-outcome>createMember</from-outcome>
        <to-activity-id>CreateInsert</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__5">
      <from-activity-id>CreateInsert</from-activity-id>
      <control-flow-case id="__6">
        <from-outcome>CreateInsert</from-outcome>
        <to-activity-id>MemberKYC</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
