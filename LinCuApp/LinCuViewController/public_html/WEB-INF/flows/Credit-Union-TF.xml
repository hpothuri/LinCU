<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="Credit-Union-TF">
    <default-activity>ManageCreditCardUnion</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <view id="CreditCardUnion">
      <page>/fragments/CreateCreditUnion.jsff</page>
    </view>
    <view id="ManageCreditCardUnion">
      <page>/fragments/ManageCreditCardUnion.jsff</page>
    </view>
    <control-flow-rule id="__1">
      <from-activity-id>ManageCreditCardUnion</from-activity-id>
      <control-flow-case id="__2">
        <from-outcome>create</from-outcome>
        <to-activity-id>CreditCardUnion</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__3">
      <from-activity-id>CreditCardUnion</from-activity-id>
      <control-flow-case id="__4">
        <from-outcome>back</from-outcome>
        <to-activity-id>ManageCreditCardUnion</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
